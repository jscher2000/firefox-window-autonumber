{
	"name": "Auto-number Windows - stable menu order",
	"description": "Keep your windows in the order they were opened on the Mac OS Window menu.",
	"version": "0.3",
	"manifest_version": 2,
	"browser_specific_settings":{
		"gecko": {
			"id": "window-autonumber@jeffersonscher.com",
			"strict_min_version": "68.0"
		}
	},
	"background": {
		"scripts": [
			"window-autonumber-background.js"
		]
	},
	"icons": {
		"128": "icons/Win123.png"
	},
	"permissions": [
		"menus"
	]
}
